{
  "description": "Docker plugin for storing volumes on custom paths",
  "documentation": "https://example.com/docker/plugin",
  "workdir": "/volumes-on-paths",
  "entrypoint": [
    "python",
    "-m",
    "flask",
    "--app",
    "plugin",
    "run",
    "--host=/run/docker/plugins/$PLUGIN_NAME.sock"
  ],
  "interface": {
    "socket": "volumes-on-paths.sock",
    "types": [
      "docker.volumedriver/1.0"
    ]
  },
  "network": {
    "type": "none"
  },
  "mounts": [
    {
        "destination": "/mnt",
        "source": "/mnt",
        "type": "bind",
        "options": [
            "bind",
            "rw"
        ]
    }
  ],
  "propagatedMount": "/var/logs/",
  "env": [
    {
      "description": "",
      "name": "USABLE_PATHS",
      "settable": [
        "value"
      ],
      "value": "*"
    }
  ],
  "linux": {
    "capabilities": [
      "CAP_SYS_ADMIN"
    ]
  }
}