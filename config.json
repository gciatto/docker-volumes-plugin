{
  "description": "Docker plugin for storing volumes on custom paths",
  "documentation": "https://example.com/docker/plugin",
  "workdir": "/volumes-on-paths",
  "entrypoint": [
    "/usr/bin/bash",
    "./entrypoint.sh",
    "python",
    "-m",
    "flask",
    "--app",
    "plugin.service",
    "run",
    "--host=unix:///run/docker/plugins/volumes-on-paths.sock"
  ],
  "interface": {
    "socket": "volumes-on-paths.sock",
    "types": [
      "docker.volumedriver/1.0"
    ]
  },
  "network": {
    "type": "host"
  },
  "mounts": [
    {
        "destination": "/var/log/",
        "source": "/var/log/",
        "type": "bind",
        "options": [
            "bind",
            "rw"
        ]
    }
  ],
  "env": [
    {
      "description": "",
      "name": "NFS_MOUNT",
      "settable": [
        "value"
      ],
      "value": "data"
    }
  ],
  "propagatedMount": "/var/lib/docker/x-drives",
  "linux": {
    "capabilities": [
      "CAP_SYS_ADMIN"
    ]
  }
}